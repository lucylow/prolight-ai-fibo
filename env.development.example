# Development Environment Variables
# Copy this file to .env.development and fill in your values

# Backend API URL
VITE_API_URL=http://localhost:8000

# Frontend commit hash (for deploy-check banner)
# This will be automatically set by CI/CD in production
# For local dev, you can set it to "dev" or leave empty
VITE_COMMIT_HASH=dev

# Supabase Configuration (if using Supabase)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# reCAPTCHA Site Key (for Contact and Careers forms)
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# CI/CD Actions URL (for deploy-check banner)
VITE_CI_ACTIONS_URL=https://github.com/your-org/your-repo/actions

# Stripe (if using Stripe for payments)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

